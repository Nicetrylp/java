package com.Nice_try;

public class Test {
	public static void main(String[] args) {
		
		System.out.println(fun(9119));
	}
	public static int fun(int number) {
		int value = 0;
		int j = 0;
		while(number > 9) {
			int a = number % 10;
			a*=a;
			if(a>10)
				j+=2;
			else
				j++;
			value =(int) (a*Math.pow(10, j))+value;
			System.out.println(value);
			number/=10;
		}
		return value;
	}
}
