package com.game2;

public class Player {
	
	private String name;
	private boolean isDie;
	// 构造函数
	public Player() {
		super();
	}
	public Player(String name) {
		super();
		this.name = name;
	}
	// get/set方法
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public boolean isDie() {
		return isDie;
	}
	public void setDie(boolean isDie) {
		this.isDie = isDie;
	}
	// 开枪
	public void Openfire() {
		
	}
}
